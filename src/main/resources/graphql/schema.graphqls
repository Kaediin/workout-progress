input UserInput {
    firstName: String!
    infix: String!
    lastName: String!
    email: String!
    password: String!

}

type User {
    id: ID!
    firstName: String!
    infix: String!
    lastName: String!
    email: String!
    password: String!

}

type Query {
    userById(id: String!): User
    userByEmail(email: String!): User
}

type Mutation {
    createUser(userInput: UserInput!): User
}