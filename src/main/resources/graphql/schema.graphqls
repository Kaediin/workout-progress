input UserInput {
    firstName: String!
    middleName: String!
    lastName: String!
    email: String!
    password: String!
}

type User {
    id: ID!
    firstName: String!
    middleName: String!
    lastName: String!
    email: String!
    #    password: String!
}

type Query {
    users: [User]
    userById(id: String!): User
}

type Mutation {
    createUser(userInput: UserInput!): User
}
